
@misc{archambault_limits_nodate,
	title = {The limits of bibliometrics for the analysis of the social sciences and humanities literature},
	url = {https://ost.openum.ca/files/sites/132/2017/06/WSSR_ArchambaultLariviere.pdf},
	abstract = {There are several limits to the use of bibliometric analysis of scholarly communication in the social sciences and humanities. This paper reviews three of those limits: the lower proportion of social science and humanities journal articles; social sciences and humanities literature’s ageing rate, and conversely its post-publication citation rate; and the local relevance of social sciences and humanities knowledge. It also discusses the choice of bibliometric databases when measuring social sciences and humanities research.},
